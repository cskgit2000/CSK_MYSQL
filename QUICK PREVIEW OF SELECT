CREATE TABLE PLAYERS_INFO (
    NAME VARCHAR(50),
    JERSEY_NO INT,
    AGE INT,
    INTER_MAT INT,
    SCORE INT
);

INSERT INTO PLAYERS_INFO(NAME,JERSEY_NO,AGE,INTER_MAT,SCORE)
VALUES("DHONI",7,43,400,15000),
("KOHLI",18,34,450,20000),
("SACHIN",10,500,29000);

