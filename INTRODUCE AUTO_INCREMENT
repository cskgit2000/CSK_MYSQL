CREATE TABLE PLAYERS_INFO (
    SI_NO INT PRIMARY KEY AUTO_INCREMENT,
    NAME VARCHAR(50),
    JERSEY_NO INT,
    AGE INT,
    INTER_MAT INT,
    SCORE INT
);

