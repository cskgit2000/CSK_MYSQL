CREATE TABLE PLAYERS_INFO (
    NAME VARCHAR(50) NOT NULL,
    JERSEY_NO INT NOT NULL,
    AGE INT NOT NULL,
    INTER_MAT INT NOT NULL,
    SCORE INT NOT NULL
);

